[user]
	name = yangchaohe
	email = manu2x@qq.com
[alias]
	lg = log --color --oneline --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	st = status
	unstage = reset HEAD
	br = branch
	ci = commit
	co = checkout
[gui]  
	encoding = utf-8  
	# 代码库统一使用utf-8  
[i18n]  
 	commitencoding = utf-8  
	# log编码  
[svn]  
	pathnameencoding = utf-8  
	# 支持中文路径  
[core]
	quotepath = false 
	# status引用路径不再是八进制（反过来说就是允许显示中文了）
